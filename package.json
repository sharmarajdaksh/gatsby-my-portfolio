{
  "name": "dakshraj-sharma",
  "private": true,
  "description": "Dakshraj Sharma's Portfolio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd --file .env.development --fallback gatsby develop gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "exit 0",
    "deploy": "gatsby build && gh-pages -d public -b master -r https://$GITHUB_TOKEN@github.com/sharmarajdaksh/sharmarajdaksh.github.io.git"
  },
  "dependencies": {
    "gatsby": "^2.20.12",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-transition-link": "^1.18.0",
    "gatsby-remark-images": "^3.2.4",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.6",
    "gsap": "^3.2.6",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "github-pages": "^3.0.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/sharmarajdaksh/gatsby-my-portfolio"
